
{
    "editor.fontFamily": "fira-code-nerd, FiraCode Nerd Font, Consolas,'CourierNew', monospace",
    "editor.fontLigatures": true,
    "workbench.colorTheme": "Bearded Theme Coffee Reversed",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.aux": true,
        "**/*.bbl": true,
        "**/*.fls": true,
        "**/*.ind": true,
        "**/*.nlo": true,
        "**/*.lot": true,
        "**/*.out": true,
        "**/*.ldx": true,
        "**/*.blg": true,
        "**/*.idx": true,
        "**/*.ilg": true,
        "**/*.lof": true,
        "**/*.toc": true,
        "**/*.fdb_latexmk": true,
        "**/*.log": true,
        "**/*.bcf": true,
        "**/*.run.xml": true,
        "**/*.acn": true,
        "**/*.glo": true,
        "**/*.ist": true
    },
    "editor.wordWrap": "bounded",
    "editor.formatOnSave": true,
    "spellright.language": [
        "pt-BR"
    ]
}
